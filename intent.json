{
    "intent":[
        {
            "label": "describe",
            "pattern": ["what is this","what is this bot","what is this chatbot for",
            "what is this thing for","what is this chatbot about","what can you do",
            "what is your name","who are you","what are you","what is your purpose"]
        },
        {
            "label": "greeting",
            "pattern": ["hello","hi","hey","greetings","how are you",
            "hiya","hey there","hi there","heya","how do you do"]
        },
        {
            "label": "goodbye",
            "pattern": ["bye","goodbye","see you","see ya","later","laters","bye bye","farewell",
            "have a nice day","catch you later","talk to you later"]

        },
        {
            "label": "thanks",
            "pattern": ["you are good at your job","you are great","well done","you were helpful"]
        },
        {
            "label": "welcome",
            "pattern": ["thanks for helping me","you were a good help","thanks for solving the problem",
            "thank you"]
        },
        {
            "label": "help",
            "pattern": ["help","help me","can you help me","can you help",
            "what can you do","what are you capable of","what are you able to do","what are you able to help with"]
        },
        {
            "label": "default",
            "pattern": ["random","this is a house","is this a helicopter",
            "this is a car","this is a boat","this is a plane","this is a train",
            "this is a bus","this is a truck"]
        }
    ]
}
